{
  "name": "upc-app",
  "version": "1.0.0",
  "description": "An Electron application with Solid and TypeScript",
  "main": "./out/main/index.js",
  "author": "example.com",
  "homepage": "https://www.electronjs.org",
  "type": "module",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config"
  },
  "dependencies": {
    "@chumsinc/gtin-tools": "^1.0.3",
    "@doeixd/create-virtualized-list-solid": "^1.3.0",
    "@duckdb/duckdb-wasm": "^1.28.1-dev106.0",
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@kobalte/core": "^0.13.6",
    "@kobalte/tailwindcss": "^0.9.0",
    "@modular-forms/solid": "^0.23.0",
    "@solid-primitives/destructure": "^0.1.17",
    "@solid-primitives/props": "^3.1.11",
    "@solid-primitives/refs": "^1.0.8",
    "@solid-primitives/scheduled": "^1.4.3",
    "@solidjs/router": "^0.14.3",
    "@tanstack/solid-table": "^8.20.5",
    "@tanstack/solid-virtual": "^3.10.7",
    "duckdb-async": "^1.0.0",
    "electron-updater": "^6.3.4",
    "epc-tds": "^1.4.1",
    "neotraverse": "^0.6.18",
    "scule": "^1.3.0",
    "solid-dzone": "^1.0.0",
    "unplugin-auto-expose": "^0.3.0"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^2.4.0",
    "@iconify/json": "^2.2.246",
    "@types/node": "^22.5.4",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "autoprefixer": "^10.4.20",
    "electron": "^32.0.2",
    "electron-builder": "^25.0.5",
    "electron-vite": "^2.3.0",
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-solid": "^0.14.3",
    "postcss": "^8.4.45",
    "prettier": "^3.3.3",
    "solid-js": "^1.8.22",
    "tailwindcss": "^3.4.10",
    "typescript": "5.5.2",
    "typescript-plugin-css-modules": "^5.1.0",
    "unplugin-icons": "^0.19.3",
    "vite": "^5.4.3",
    "vite-css-modules": "^1.4.2",
    "vite-plugin-pages": "^0.32.3",
    "vite-plugin-solid": "^2.10.2"
  }
}
